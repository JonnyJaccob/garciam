{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Prueba/git-man.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Prueba/git-man.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/Prueba/git-man.csproj",
                "/property:GenerateFullPaths=true",
                "/consoleloggerparameters:NoSummary"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "JC",
            "command": "java",
            "windows": {
                "command": "C:\\Program Files\\Java\\jdk1.8.0_251\bin\\javac.exe"
            },
            "args": [
                "${file}"
            ]
        },
        {
            "label": "JR",
            "command": "java",
            "windows": {
                "command": "C:\\Program Files\\Java\\jdk1.8.0_251\bin\\java.exe"
            },
            "args": [
                "${fileBasenameNoExtension}"
            ]
        }
    ]
}